
  <div class="act {{ actClass }}">
    {% if actClass == "headliner" %}
    <p class="stamp stamp-act-label">Featuring</p>
    {% elseif actClass == "opener"  %}
    <p class="stamp stamp-act-label">With support from</p>
    {# {% else %}
    <p class="stamp stamp-act-label">and</p> #}
    {% endif %}
    {% if act.image %}
    <img src="/images/acts/{{ act.image }}" />
    {% else %}
    <img src="/images/acts/mystery.jpg" />
    {% endif  %}
    <h3 class="act-name">{{ act.name }}</h3>
  </div>
