<div class="section">
{% for gig in collections.show | reverse %}
  {% if loop.first %}
  <h2 class="stamp">Coming up</h2>
  {% endif %}
  <p class="date">8pm, {{ gig.date | dateDisplay }}</p>
  <h2><a href="{{ gig.url }}">{{ gig.data.title}}</a></h2>
  {% if loop.first %}
</div>
<div class="section">
  <h2 class="stamp">Memory lane</h2>
  {% endif %}
{% endfor %}
</div>

